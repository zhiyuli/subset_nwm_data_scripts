netcdf {%filename%} {
dimensions:
	feature_id = {%feature_id%} ;
	time = UNLIMITED ; // (1 currently)
	reference_time = 1 ;
variables:
	int time(time) ;
		time:long_name = "valid output time" ;
		time:standard_name = "time" ;
		time:units = "minutes since 1970-01-01 00:00:00 UTC" ;
		time:_Storage = "chunked" ;
		time:_DeflateLevel = 2 ;
		time:_Shuffle = "true" ;
		time:_Endianness = "little" ;
	int reference_time(reference_time) ;
		reference_time:long_name = "model initialization time" ;
		reference_time:standard_name = "forecast_reference_time" ;
		reference_time:units = "minutes since 1970-01-01 00:00:00 UTC" ;
		reference_time:_Storage = "chunked" ;
		reference_time:_DeflateLevel = 2 ;
		reference_time:_Shuffle = "true" ;
		reference_time:_Endianness = "little" ;
	int feature_id(feature_id) ;
		feature_id:long_name = "Lake COMMOND ID" ;
		feature_id:comment = "ComID from NHDPlusV2 waterbody layer" ;
		feature_id:cf_role = "timeseries_id" ;
		feature_id:_Storage = "chunked" ;
		feature_id:_DeflateLevel = 2 ;
		feature_id:_Shuffle = "true" ;
		feature_id:_Endianness = "little" ;
	float latitude(feature_id) ;
		latitude:long_name = "Lake latitude" ;
		latitude:standard_name = "latitude" ;
		latitude:units = "degrees_north" ;
		latitude:_Storage = "chunked" ;
		latitude:_DeflateLevel = 2 ;
		latitude:_Shuffle = "true" ;
		latitude:_Endianness = "little" ;
	float longitude(feature_id) ;
		longitude:long_name = "Lake longitude" ;
		longitude:standard_name = "longitude" ;
		longitude:units = "degrees_east" ;
		longitude:_Storage = "chunked" ;
		longitude:_DeflateLevel = 2 ;
		longitude:_Shuffle = "true" ;
		longitude:_Endianness = "little" ;
	float elevation(feature_id) ;
		elevation:long_name = "Water Surface Elevation" ;
		elevation:units = "meters" ;
		elevation:_Storage = "chunked" ;
		elevation:_DeflateLevel = 2 ;
		elevation:_Shuffle = "true" ;
		elevation:_Endianness = "little" ;
	int inflow(time, feature_id) ;
		inflow:_FillValue = -999900 ;
		inflow:missing_value = -999900 ;
		inflow:long_name = "Lake Inflow" ;
		inflow:units = "m3 s-1" ;
		inflow:coordinates = "latitude longitude" ;
		inflow:scale_factor = 0.01f ;
		inflow:add_offset = 0.f ;
		inflow:valid_range = -1000000, 1000000 ;
		inflow:_Storage = "chunked" ;
		inflow:_DeflateLevel = 2 ;
		inflow:_Shuffle = "true" ;
		inflow:_Endianness = "little" ;
	int outflow(time, feature_id) ;
		outflow:_FillValue = -999900 ;
		outflow:missing_value = -999900 ;
		outflow:long_name = "Lake Outflow" ;
		outflow:units = "m3 s-1" ;
		outflow:coordinates = "latitude longitude" ;
		outflow:scale_factor = 0.01f ;
		outflow:add_offset = 0.f ;
		outflow:valid_range = -1000000, 1000000 ;
		outflow:_Storage = "chunked" ;
		outflow:_DeflateLevel = 2 ;
		outflow:_Shuffle = "true" ;
		outflow:_Endianness = "little" ;

// global attributes:
		:featureType = "timeSeries" ;
		:proj4 = "+proj=longlat +datum=NAD83 +no_defs" ;
		:model_initialization_time = "{%model_initialization_time%}" ;
		:station_dimension = "lake_id" ;
		:model_output_valid_time = "{%model_output_valid_time%}" ;
		:esri_pe_string = "GEOGCS[GCS_North_American_1983,DATUM[D_North_American_1983,SPHEROID[GRS_1980,6378137.0,298.257222101]],PRIMEM[Greenwich,0.0],UNIT[Degree,0.017453292519943295]]" ;
		:Conventions = "CF-1.6" ;
		:model_version = "NWM 1.2" ;
		:_SuperblockVersion = 2 ;
		:_IsNetcdf4 = 1 ;
		:_Format = "netCDF-4" ;
}
